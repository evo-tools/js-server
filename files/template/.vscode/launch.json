{
	"version": "0.0.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "debugger",
			"args": ["${workspaceFolder}/src/main.ts"],
			"runtimeArgs": ["--nolazy", "-r", "tsconfig-paths/register", "-r", "ts-node/register"],
			"sourceMaps": true,
			"smartStep": true,
			"cwd": "${workspaceRoot}",
			"protocol": "inspector",
			"console": "integratedTerminal"
		}
	]
}
